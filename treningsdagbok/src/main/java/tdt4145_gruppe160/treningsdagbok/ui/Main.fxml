<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tdt4145_gruppe160.treningsdagbok.ui.MainController">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
          	<MenuItem mnemonicParsing="false" text="New workout" onAction="#onNewWorkout" />
            <MenuItem mnemonicParsing="false" text="New bodyweight exercise" onAction="#onNewBodyeightExercise" />
            <MenuItem mnemonicParsing="false" text="New equipment exercise" onAction="#onNewEquipmentExercise" />
            <MenuItem mnemonicParsing="false" text="New exercise group" onAction="#onNewExerciseGroup" />
            <MenuItem mnemonicParsing="false" text="New equipment" onAction="#onNewEquipment" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Recent workouts" onAction="#onRecentWorkouts" />
            <MenuItem mnemonicParsing="false" text="Show results" onAction="#onResults" />
            <MenuItem mnemonicParsing="false" text="Show exercises by group" onAction="#onExercicesByGroup" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane fx:id="anchorPane" maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
      </children>
    </AnchorPane>
  </children>
</VBox>
